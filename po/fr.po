#: argparse.py:226
#, python-format
msgid "%(heading)s:"
msgstr "%(heading)s :"

#: argparse.py:297 argparse.py:296
msgid "usage: "
msgstr "usage : "

#: argparse.py:673
#, python-format
msgid " (default: %(default)s)"
msgstr "(defaut : %(default)s)"

#: argparse.py:735
#, python-format
msgid "argument %(argument_name)s: %(message)s"
msgstr "argument %(argument_name)s : %(message)s"

#: argparse.py:844 argparse.py:854
msgid ".__call__() not defined"
msgstr ".__call__() n'est pas défini"

#: argparse.py:1135
msgid "show program's version number and exit"
msgstr "affiche la version du programme et termine l'exécution"

#: argparse.py:1198
#, python-format
msgid "conflicting subparser: %s"
msgstr "subparser en conflit : %s"

#: argparse.py:1202
#, python-format
msgid "conflicting subparser alias: %s"
msgstr "alias de subparser en conflit : %s"

#: argparse.py:1241 argparse.py:1197
#, python-format
msgid "unknown parser %(parser_name)r (choices: %(choices)s)"
msgstr "parser inconnu %(parser_name)r (choix : %(choices)s)"

#: argparse.py:1245
#, python-format
msgid "command '%(parser_name)s' is deprecated"
msgstr "la commande '%(parser_name)s' est dépréciée"

#: argparse.py:1306 argparse.py:1257
#, python-format
msgid "argument \"-\" with mode %r"
msgstr "argument \"-\" avec mode %r"

#: argparse.py:1315 argparse.py:1266
#, python-format
msgid "can't open '%(filename)s': %(error)s"
msgstr "impossible d'ouvrir '%(filename)s': %(error)s"

#: argparse.py:1526 argparse.py:1475
#, python-format
msgid "cannot merge actions - two groups are named %r"
msgstr "impossible d'ajouter les actions - deux groupes ont pour nom %r"

#: argparse.py:1568 argparse.py:1513
msgid "'required' is an invalid argument for positionals"
msgstr "'required' est invalide pour les arguments positionnels"

#: argparse.py:1589 argparse.py:1535
#, python-format
msgid ""
"invalid option string %(option)r: must start with a character "
"%(prefix_chars)r"
msgstr ""
"option %(option)r invalide : doit commencer par un caractère "
"%(prefix_chars)r"

#: argparse.py:1607 argparse.py:1553
#, python-format
msgid "dest= is required for options like %r"
msgstr "dest= est nécessaire pour des options comme %r"

#: argparse.py:1624 argparse.py:1570
#, python-format
msgid "invalid conflict_resolution value: %r"
msgstr "la valeur de résolution de conflit est invalide : %r"

#: argparse.py:1642 argparse.py:1588
#, python-format
msgid "conflicting option string: %s"
msgid_plural "conflicting option strings: %s"
msgstr[0] "option en conflit : %s"
msgstr[1] "options en conflit : %s"

#: argparse.py:1717 argparse.py:1654
msgid "mutually exclusive arguments must be optional"
msgstr "des arguments mutuellement exclusifs doivent être optionnels"

#: argparse.py:1794 argparse.py:1721
msgid "positional arguments"
msgstr "arguments positionnels"

#: argparse.py:1795
msgid "options"
msgstr "options"

#: argparse.py:1810 argparse.py:1737
msgid "show this help message and exit"
msgstr "affiche ce message et termine le programme"

#: argparse.py:1839 argparse.py:1768
msgid "cannot have multiple subparser arguments"
msgstr "il n'est pas possible d'avoir plusieur arguments de subparser"

#: argparse.py:1845
msgid "subcommands"
msgstr "sous commandes"

#: argparse.py:1891 argparse.py:2454 argparse.py:1820 argparse.py:2331
#, python-format
msgid "unrecognized arguments: %s"
msgstr "arguments non reconnus : %s"

#: argparse.py:1997 argparse.py:1921
#, python-format
msgid "not allowed with argument %s"
msgstr "interdit avec l'argument %s"

#: argparse.py:2016 argparse.py:2210
#, python-format
msgid "ambiguous option: %(option)s could match %(matches)s"
msgstr "option ambiguë : %(option)s pourrait correspondre à %(matches)s"

#: argparse.py:2048 argparse.py:2080 argparse.py:1967 argparse.py:1981
#, python-format
msgid "ignored explicit argument %r"
msgstr "argument explicite %r ignoré"

#: argparse.py:2100
#, python-format
msgid "option '%(option)s' is deprecated"
msgstr "l'option '%(option)s' est dépréciée"

#: argparse.py:2132
#, python-format
msgid "argument '%(argument_name)s' is deprecated"
msgstr "l'argument '%(argument_name)s' est déprécié"

#: argparse.py:2225 argparse.py:2088
#, python-format
msgid "the following arguments are required: %s"
msgstr "les arguments suivants sont exigés : %s"

#: argparse.py:2240 argparse.py:2103
#, python-format
msgid "one of the arguments %s is required"
msgstr "un des arguments %s est exigé"

#: argparse.py:2284 argparse.py:2146
msgid "expected one argument"
msgstr "un argument est attendu"

#: argparse.py:2285 argparse.py:2147
msgid "expected at most one argument"
msgstr "au plus un argument est attendu"

#: argparse.py:2286 argparse.py:2148
msgid "expected at least one argument"
msgstr "au moins un argument est attendu"

#: argparse.py:2290 argparse.py:2152
#, python-format
msgid "expected %s argument"
msgid_plural "expected %s arguments"
msgstr[0] "%s argument attendu"
msgstr[1] "%s arguments attendus"

#: argparse.py:2400 argparse.py:2274
#, python-format
msgid "unexpected option string: %s"
msgstr "option inattendue : %s"

#: argparse.py:2536 argparse.py:2471
#, python-format
msgid "%r is not callable"
msgstr "%r n'est pas un \"callable\""

#: argparse.py:2552 argparse.py:2488
#, python-format
msgid "invalid %(type)s value: %(value)r"
msgstr "valeur %(type)s invalide : %(value)r"

#: argparse.py:2567 argparse.py:2499
#, python-format
msgid "invalid choice: %(value)r (choose from %(choices)s)"
msgstr "choix invalide : %(value)r (choix parmi %(choices)s)"

#: argparse.py:2645 argparse.py:2575
#, python-format
msgid "%(prog)s: error: %(message)s\n"
msgstr "%(prog)s : erreur : %(message)s\n"

#: argparse.py:2649
#, python-format
msgid "%(prog)s: warning: %(message)s\n"
msgstr "%(prog)s : avertissement : %(message)s\n"

#: argparse.py:1722
msgid "optional arguments"
msgstr "arguments optionnels"
