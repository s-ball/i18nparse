#: argparse.py:299
msgid "usage: "
msgstr "usage : "

#: argparse.py:678
#, python-format
msgid " (default: %(default)s)"
msgstr ""

#: argparse.py:740
#, python-format
msgid "argument %(argument_name)s: %(message)s"
msgstr ""

#: argparse.py:1115
msgid "show program's version number and exit"
msgstr "affiche la version du programme et termine l'exécution"

#: argparse.py:1224
#, python-format
msgid "unknown parser %(parser_name)r (choices: %(choices)s)"
msgstr "parser inconnu %(parser_name)r (choix : %(choices)s"

#: argparse.py:1228
#, python-format
msgid "command '%(parser_name)s' is deprecated"
msgstr "la commande '%(parser_name)s' est dépréciée"

#: argparse.py:1295
#, python-format
msgid "argument \"-\" with mode %r"
msgstr "argument \"-\" avec mode %r"

#: argparse.py:1304
#, python-format
msgid "can't open '%(filename)s': %(error)s"
msgstr "impossible d'ouvrir '%(filename)s': %(error)s"

#: argparse.py:1639
#, python-format
msgid "conflicting option string: %s"
msgid_plural "conflicting option strings: %s"
msgstr[0] "option en conflit : %s"
msgstr[1] "options en conflit : %s"

#: argparse.py:1815
msgid "positional arguments"
msgstr "arguments positionnels"

#: argparse.py:1816
msgid "options"
msgstr "options"

#: argparse.py:1831
msgid "show this help message and exit"
msgstr "affiche ce message et termine le programme"

#: argparse.py:1866
msgid "subcommands"
msgstr "sous commandes"

#: argparse.py:1913 argparse.py:2476
#, python-format
msgid "unrecognized arguments: %s"
msgstr "arguments non reconnus : %s"

#: argparse.py:2019
#, python-format
msgid "not allowed with argument %s"
msgstr "interdit avec l'argument %"

#: argparse.py:2038
#, python-format
msgid "ambiguous option: %(option)s could match %(matches)s"
msgstr "option ambiguë : %(option)s pourrait correspondre à %(matches)s"

#: argparse.py:2070 argparse.py:2102
#, python-format
msgid "ignored explicit argument %r"
msgstr "argument explicite %r ignoré"

#: argparse.py:2122
#, python-format
msgid "option '%(option)s' is deprecated"
msgstr "l'option '%(option)s' est dépréciée"

#: argparse.py:2154
#, python-format
msgid "argument '%(argument_name)s' is deprecated"
msgstr "l'argument '%(argument_name)s' est déprécié"

#: argparse.py:2247
#, python-format
msgid "the following arguments are required: %s"
msgstr "les arguments suivants sont exigés : %s"

#: argparse.py:2262
#, python-format
msgid "one of the arguments %s is required"
msgstr "un des arguments %s est exigé"

#: argparse.py:2306
msgid "expected one argument"
msgstr "un argument est attendu"

#: argparse.py:2307
msgid "expected at most one argument"
msgstr "au plus un argument est attendu"

#: argparse.py:2308
msgid "expected at least one argument"
msgstr "au moins un argument est attendu"

#: argparse.py:2312
#, python-format
msgid "expected %s argument"
msgid_plural "expected %s arguments"
msgstr[0] "%s argument attendu"
msgstr[1] "%s arguments attendus"

#: argparse.py:2422
#, python-format
msgid "unexpected option string: %s"
msgstr "option inattendue : %s"

#: argparse.py:2569
#, python-format
msgid "invalid %(type)s value: %(value)r"
msgstr "valeur %(type)s invalide : %(value)r"

#: argparse.py:2587
#, python-format
msgid "invalid choice: %(value)r (choose from %(choices)s)"
msgstr "choix invalide : %(value)r (choix parmi %(choices)s"

#: argparse.py:2595
#, fuzzy, python-format
msgid ""
"invalid choice: %(value)r, maybe you meant %(closest)r? (choose from "
"%(choices)s)"
msgstr "choix invalide : %(value)r, peut être vouliez-vous %(closest)r"
"? (choisissez parmi %(choices)s)"

#: argparse.py:2675
#, python-format
msgid "%(prog)s: error: %(message)s\n"
msgstr "%(prog)s : erreur : %(message)s\n"

#: argparse.py:2679
#, python-format
msgid "%(prog)s: warning: %(message)s\n"
msgstr "%(prog)s : avertissement : %(message)s\n"
